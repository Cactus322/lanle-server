{
  "compilerOptions": {
    "target": "ES6",
    "outDir": "./build/",
    "module": "commonjs",
    "strict": true,
    "noUnusedLocals": false, //develop
    "noUnusedParameters": false, //develop
    "noImplicitReturns": false, //develop
    "noImplicitAny": true, //develop
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
  }
}